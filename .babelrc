{
  "presets": ["react", ["env", {
    "targets": {
      "browsers": [
        "last 2 versions",
        "not ie <= 11",
        "not android <= 56"
      ]
    },
    "loose": true,
    "modules": "commonjs",
    "useBuiltIns": false
  }]],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "docs": {
      "presets": [
        ["env", {
          "targets": {
            "uglify": true
          }
        }]
      ],
      "plugins": [
        "transform-class-properties"
      ],
    },
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {
          "mode": "wrap",
          "ignoreFilenames": ["node_modules"]
        }]]
    }
  }
}
